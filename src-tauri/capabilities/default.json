{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "window:allow-set-focus",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "tray:allow-new",
    "tray:allow-set-title",
    "shell:allow-open",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-clear",
    "dialog:allow-ask",
    "dialog:default",
    "dialog:allow-confirm"
  ]
}
